{
  "name": "suspect-x-web",
  "version": "0.0.2",
  "description": "This is web service version of Suspect-X project",
  "productName": "Suspect-X-Web",
  "author": "Hsieh-Yu-Hung <yuhunghsieh@accuinbio.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue ./",
    "format": "prettier --write \"**/*.{js,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "ENV_PATH=.env.development quasar dev",
    "build": "quasar build",
    "deploy": "ENV_PATH=.env.production quasar build && firebase deploy",
    "emulate:prod": "ENV_PATH=.env.production quasar build && firebase emulators:start",
    "emulate:dev": "ENV_PATH=.env.development quasar build --debug && firebase emulators:start"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.4",
    "core-js": "^3.31.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "firebase": "^11.1.0",
    "firebase-admin": "^13.0.2",
    "loglevel": "^1.9.2",
    "moment": "^2.30.1",
    "pinia": "^2.3.0",
    "quasar": "^2.16.0",
    "vue": "^3.4.18",
    "vue-router": "^4.0.12",
    "vuex": "^4.0.2",
    "vuex-persistedstate": "^4.1.0",
    "winston": "^3.17.0",
    "winston-browser": "^1.0.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.13.14",
    "@quasar/app-webpack": "^3.13.0",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^9.0.0",
    "eslint-webpack-plugin": "^3.1.1",
    "prettier": "^2.5.1"
  },
  "browserslist": [
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
  "engines": {
    "node": "22",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
